<?xml version="1.0"?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <deployment>
        <dependencies>
             <!-- En Sistramit Hibernate requiere javassist: parece bug 1288251  -->
            <module name="org.javassist" export="true" slot="main" />
            <!-- Dependencias integra (plugin validacion firmas)-->
            <module name="org.bouncycastle" export="true" slot="main" />
            <module name="org.apache.xerces" export="true" slot="main"/>
            <module name="com.sun.xml.messaging.saaj" export="true" slot="main"/>
        </dependencies>
    </deployment>
</jboss-deployment-structure>
