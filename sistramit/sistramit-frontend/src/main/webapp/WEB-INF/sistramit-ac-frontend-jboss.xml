<?xml version="1.0" encoding="UTF-8"?>
<beans	xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="
				http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
				http://www.springframework.org/schema/context
        		http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<import resource="classpath:sistramit-ejb-core-api.xml" />

	<bean id="negocioModuleConfig" class="es.caib.sistramit.frontend.model.comun.ModuleConfig">
		<property name="version" value="${sistra2.version}" />
		<property name="commitSvn" value="${prefix.revision}" />
		<property name="commitGit" value="${git.commit.id.abbrev}" />
	</bean>

</beans>
